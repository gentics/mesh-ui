{
  "name": "mesh-ui",
  "version": "0.10.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-original": "ng serve",
    "start": "ng serve --verbose --proxy-config proxy.conf.json",
    "build": "ng build --prod",
    "generate-models": "node config/build-models-from-raml",
    "test": "ng test",
    "test-no-sourcemaps": "ng test -sm=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "preinstall": "node ./check-if-yarn-is-used",
    "e2e": "ng e2e --proxy-config proxy.conf.json",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again"
  },
  "lint-staged": {
    "concurrent": false,
    "subTaskConcurrency": 1,
    "linters": {
      "*.ts": [
        "tslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.html": [
        "js-beautify -r",
        "git add"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.1.3",
    "@angular/router": "^5.0.0",
    "@angularclass/conventions-loader": "^1.0.13",
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@ngx-translate/core": "^9.0.2",
    "@types/quill": "0.0.31",
    "core-js": "^2.4.1",
    "gentics-ui-core": "^6.1.1",
    "gentics-ui-image-editor": "^0.3.5",
    "http-server": "^0.10.0",
    "ie-shim": "^0.1.0",
    "immutablets": "^0.11.0",
    "monaco-editor": "^0.10.1",
    "ngx-pagination": "^3.1.0",
    "quill": "^1.3.4",
    "quill-table": "^1.0.0",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/ramda": "^0.25.21",
    "@types/request-promise": "^4.1.41",
    "codelyzer": "^4.0.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "js-beautify": "^1.7.5",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.0.5",
    "mesh-model-generator": "^0.3.1",
    "prettier": "^1.12.1",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.8.0",
    "typescript": "~2.4.2",
    "uuid-random": "^1.0.6"
  }
}
