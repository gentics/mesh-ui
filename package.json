{
  "name": "mesh-ui",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start-original": "ng serve",
    "start": "ng serve --verbose --proxy-config proxy.conf.json --live-reload true",
    "build": "ng build --prod",
    "generate-models": "node config/build-models-from-raml",
    "test": "ng test",
    "test-no-sourcemaps": "ng test -sm=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e-protractor": "ng e2e",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "mesh": "docker run --rm -p '8080:8080' -e MESH_ELASTICSEARCH_URL=null -e MESH_ELASTICSEARCH_START_EMBEDDED=false gentics/mesh-demo",
    "e2e": "cross-env MESHUI_URL=http://localhost:4200 testcafe",
    "e2e-watch": "cross-env MESHUI_URL=http://localhost:4200 testcafe --live",
    "e2e-headless": "cross-env MESHUI_URL=http://localhost:4200 testcafe 'chrome:headless --window-size=1920x1080'"
  },
  "lint-staged": {
    "concurrent": false,
    "subTaskConcurrency": 1,
    "linters": {
      "*.ts": [
        "tslint --fix",
        "prettier --write",
        "git add"
      ],
      "*.html": [
        "js-beautify -r",
        "git add"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/platform-server": "^7.1.3",
    "@angular/router": "^7.0.0",
    "@angularclass/conventions-loader": "^1.0.13",
    "@angularclass/hmr": "^2.1.3",
    "@angularclass/hmr-loader": "^3.0.4",
    "@ngx-translate/core": "^9.0.2",
    "@types/quill": "0.0.31",
    "@types/sortablejs": "^1.7.2",
    "copy-to-clipboard": "^3.2.0",
    "core-js": "^2.4.1",
    "cross-env": "^5.2.0",
    "gentics-ui-core": "^7.5.1",
    "gentics-ui-image-editor": "^0.4.0",
    "http-server": "^0.10.0",
    "ie-shim": "^0.1.0",
    "immutablets": "^0.11.0",
    "jwt-decode": "^2.2.0",
    "material-design-icons-iconfont": "^5.0.1",
    "monaco-editor": "^0.10.1",
    "ngx-cookie-service": "^2.2.0",
    "ngx-pagination": "^3.1.0",
    "node-sass": "^4.12.0",
    "primeicons": "^2.0.0",
    "primeng": "^8.0.2",
    "quill": "^1.3.6",
    "quill-table": "^1.0.0",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.5.1",
    "testcafe": "^1.5.0",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^6.0.118",
    "@types/ramda": "^0.25.21",
    "@types/request-promise": "^4.1.41",
    "codelyzer": "^5.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "js-beautify": "^1.7.5",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.1",
    "lint-staged": "^7.0.5",
    "mesh-model-generator": "^0.3.1",
    "prettier": "^1.12.1",
    "protractor": "^6.0.0",
    "raw-loader": "^0.5.1",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.8.0",
    "typescript": "~3.2.1",
    "uuid-random": "^1.0.6",
    "webdriver-manager": "^13.0.0"
  }
}
