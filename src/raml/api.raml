            description: Load the node or the node's binary data which is located using the provided path.
            queryParameters: 
                cropx: 
                    description: Set image crop area x coordinate.
                    type: number
                    required: false
                    repeat: false
                    example: "260"
                croph: 
                    description: Set image crop area height.
                    type: number
                    required: false
                    repeat: false
                    example: "35"
                cropy: 
                    description: Set image crop area y coordinate.
                    type: number
                    required: false
                    repeat: false
                    example: "260"
                width: 
                    description: Set image target width. The height will automatically be calculated if the width was omitted.
                    type: number
                    required: false
                    repeat: false
                    example: "1280"
                height: 
                    description: Set image target height. The width will automatically be calculated if the height was omitted.
                    type: number
                    required: false
                    repeat: false
                    example: "720"
                cropw: 
                    description: Set image crop area width.
                    type: number
                    required: false
                    repeat: false
                    example: "35"
